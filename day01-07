import java.util.Scanner;
public class NightClass {
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        double[] score = new double[10];
        int i = 0;
        int j = 0;
        double total = 0;
        double tmp = 0;
        for(i = 0; i < score.length; i++){
            System.out.println("请输入第" + (i + 1) + "个学生的成绩");
            score[i] = scan.nextDouble();
        }
        for(i = 0; i < score.length; i++){
            total += score[i];
        }
        System.out.println("总分为" + total + ",平均分为" + (total / score.length));

        for(i = 0; i < score.length - 1; i++){
            for(j = 0; j < score.length - 1 - i; j++){
                if(score[i] > score[i + 1]){
                    score[i] = score[i + 1];
                    tmp = score[i];
                    score[i + 1] = tmp;
                }
            }
        }
        System.out.println(score[score.length - 1]);
    }
}
package day06;

public class Test {
    public static void main(String[] args){
        say();
        sayHi("Anna");
        sayHello("Anna", 14);
        double account = getNum();
        int num = plus(4, 33);
        System.out.println("获取到的数字是:" + account);
        System.out.println("4 + 33 = " + num);
        int[] arr1 = {2, 555, 666, 777, 88, 5565, 777, 44343, 65665};
        int min = getMinOfArray(arr1);
        System.out.println("数组中的最小值为:" + min );
    }
    public static void say(){
        System.out.println("My name is Cate");
    }
    public static void sayHi(String arr){
        System.out.println("My name is " + arr);
    }
    public static void sayHello(String arr,int age){
        System.out.println("My name is " + arr + ", I'm " + age + "years old.");
    }
    public static double getNum(){
        return 8.88;
    }
    public static int plus(int num1, int num2){
       return num1 + num2;
    }
    public static int getMinOfArray(int arr[]){
        int min = arr[0];
        for(int i = 1; i < arr.length; i++){
            if(arr[i] < min){
                min = arr[i];
            }
        }
        return min;
    }

}
package ooday01;


public class Aoo {
    void show(){}
    void show(String name){}
    void show(int age){}
    void show(String name, int age){}
    void show (int age,String name){}

}
package ooday01;

public class OverLoadDemo {
    public static void main(String[] args){
        Aoo o = new Aoo();
        int oe = 33;
        o.show(oe);
    }
}
package ooday01;

public class Student {
    String name;
    int age;
    String address;
    void study(){
            System.out.println(name + " study now");
        }

    void sayHi(){
        System.out.println("Hello,my name is " + name + ", I'm "
                                               + age + " years old ,my address is "
                                               + address );
    }

}
package ooday01;

public class StudentTest {
    public static void main(String[] args){
        Student ww = new Student();
        ww.name = "cookies";
        ww.age = 7;
        ww.address = "shengweiboyu";
        ww.study();
        ww.sayHi();
    }
}

    }
}
