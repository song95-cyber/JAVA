        }
        System.out.println("right");

         Scanner scan = new Scanner(System.in);
         int number = (int)(Math.random() * 1000) + 1;
         int guess;
         do{
             System.out.println(number);
             guess = scan.nextInt();
             if(guess > number){
                 System.out.println("bigger");
             }else if(guess < number){
                 System.out.println("smaller");
             }else{
                 System.out.println("right");
                 break;
             }
         }while(true);

        short a = 91;
        String name = "WKJ";
