    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        Random ran = new Random();
        int num = ran.nextInt(1001) + 1;
        int guess;
        do {
            guess = scan.nextInt();
            if (guess == num) {
                System.out.println("right");
                break;
            } else if (guess > num) {
                System.out.println("bigger");
            } else {
                System.out.println("smaller");
            }
        } while (true);


    }
