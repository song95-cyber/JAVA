package day02;

public class HomeWork {
    public static void main(String[] args) {
        //1.变量的练习：
        //声明：
        int age = 102;
        int age1 = 110, age2 = 112, age3 = 114;//声明一个变量，一次声明多个变量


        int number = 33;
        number = 44;////声明变量直接初始化，先声明变量，而后在给变量初始化

        int g = 5;
        int h = g + 10;
        System.out.println(h);//声明整型变量g，生命另一个整型变量h并赋值为h + 10,输出变量h的值。


        int i = 10;
        i = i + 10;
        System.out.println(i);//声明整型变量i，在i本身基础上增10，输出变量i的值。

        /*
        正确的变量名
        int i = 1;
        int age = 10;
        int number = 22;
        int age$ = 223;
        int age3 = 333;
        int $age = 3333;
         */

        /*
        错误的变量名
        int 3age = 33;
        int int = 33;
        int public = 33;
        int static = 33;
         */


        //2.基本数据类型的练习：int

        //声明初始化两个以上整数变量，并且输出两个证书变量的除法结果。
        int score = 33, score2 = 33, score3 =44;
        System.out.println(score3 / (score2 + score));


        //声明两个很大的变量，然后相加，输出运算结果，测试运算溢出结果。
        int a = 243333333;
        int b = 333333333;
        System.out.println(a + b);

        double ab = 3.22;
        int ba = 122;
        System.out.println(ba / ab);
        System.out.println(5 / 2);
        System.out.println(5.0 / 2);
        System.out.println(5 / 2.0);

        //3.基本数据类型的练习：long
        //声明初始化两个以上的长整型变量
        long aaa = 3000000000L;
        long bbb = 400000000000L;
        long ccc = 40349343232L;

        //声明变量存储几个较大数据的运算，演示：若有可能溢出建议将L设计在第一个数字后
        int aaaa = 993232323;
        int bbbb = 934343422;
        int cccc = aaaa + bbbb;
        long f = 10000000000L * 3 * 10;
        long ff = 10000000000L * 3 * 10;
        System.out.println(cccc);


        // 声明初始化两个以上的浮点型变量
        double dddd = 3.22222;
        double eeee = 4.22344;
        double ffff = 323.44455656;

        //声明几个浮点型变量并将她们做数学操作，演示double运算的舍入误差问题
        double gggg = 15.3;
        double hhhh = 20.0;
        System.out.println(hhhh - gggg);//这里证明了有可能发生舍入误差

        //5.基本数据类型的练习：Boolean

        //声明初始化两个以上的布尔型变量
        boolean C1 = true;
        boolean C2 = false;
        //boolean c3 = 25;//编译错误，布尔型只能赋值为true或者false


        //6.基本数据类型的练习：char

        //声明初始化五个以上的字符型变量
        char c1 = 'n';
        char c2 = '#';
        char c3 = '5';
        char c4 = '人';
        char c5 = '3';

        //声明字符型变量，演示转义符
        char c6 = '\'';
        char c7 = '\\';
        char c8 = 88;

        //7.基本数据类型的转换：两种方式

        //声明几个变量，演示自动类型转换和强制类型转换的语法
        long vvvv = 3333333333L;
        int vvvvv = (int)vvvv;

        int  vvvvvv = 33333333;
        long vvvvvvv = vvvvvv;

        //声明一个较大精度的浮点型变量，演示强转可能会发生的丢失精度问题

        double bbbbbbb = 333.223432424;
        float ddddddd = (float)bbbbbbb;

        //声明一个较大类型的长整型变量，演示强转可能会发生的溢出问题

        long vbbbb = 4444444444L;
        int bvvvv = (int)vbbbb;

        //基本数据类型的转换：两点规则

        //声明两个byte型变量b1 和 b2，并赋值为直接量

        byte bvdfefdf = 4;
        byte dgdgdgdg = 2;
        byte dfdfdffeee = (byte)(bvdfefdf + dgdgdgdg);

        //声明一个byte型变量，并赋值为b1 和 b2 的和。

        System.out.println(2 + 2);
        System.out.println(2 + '2');
        System.out.println('2' + '2');







    }
}
