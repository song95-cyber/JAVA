package day05;

public class ForDemo {
    public static void main(String[] args){
        int num = 0;
        for(num = 1; num < 10; num++){
            if(num == 6){
                break;
            }
            if(num == 3){
                continue;
            }
            System.out.println(num + "* 9 = " + num * 9);
        }
        for(num = 1; num < 10; num += 2){
            System.out.println(num + "* 9 =" + num * 9);
        }
        for(num = 0; num < 5; num++){
            System.out.println("行动是成功的阶梯");

        }
    }
}
